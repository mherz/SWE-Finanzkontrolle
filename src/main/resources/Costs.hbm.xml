<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.dhbw.tinf11b2.ofk.model.pojo">
	<class name="Costs" table="Costs">
		<id name="costsId" column="Costs_Id"> <generator class="native"/></id>
		<property name="value" column="Costs_Value"/>
		<property name="description" column="Costs_Description"/>
		<property name="timestamp" type="timestamp" column="Costs_Timestamp"/>	
		<property name="accountId" column="Account_Id"/>	
	</class>
	
	
</hibernate-mapping>