<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.dhbw.tinf11b2.ofk.model.pojo">
	<class name="Account" table="Account">
		<id name="accountId" column="Account_Id">
			<generator class="native" />
		</id>
		<property name="name" column="Account_Name" />
		<property name="value" column="Account_Value" />
		<property name="userId" column="User_Id"></property>
<!-- 	<many-to-one name="userId" column="User_Id"
			class="de.dhbw.tinf11b2.ofk.model.pojo.User" fetch="select"></many-to-one>-->
	</class>
</hibernate-mapping>