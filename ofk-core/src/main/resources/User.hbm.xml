<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.dhbw.tinf11b2.ofk.pojo">
	<class name="UserDAO" table="User">
		<id name="userId" column="User_Id"> <generator class="native"/></id>
		<property name="name" column="User_Name"/>
		<property name="email" column="User_Email"/>
		<property name="password" column="User_Password"/>
	</class>
	
	
</hibernate-mapping>